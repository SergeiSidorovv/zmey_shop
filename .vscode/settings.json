{
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "files.associations": {
        "**/templates/*/*.html": "django-html",
        "**/templates/*/*/*.html": "django-html",
    },
    "django.snippets.exclude": [
        "cms",
        "wagtail"
    ],

    "[django-html]": {
        "editor.defaultFormatter": "batisteo.vscode-django",
        "breadcrumbs.showClasses": true,
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        }
    }
}